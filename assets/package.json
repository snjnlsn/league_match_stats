{
  "devDependencies": {
    "esbuild": "^0.20.0",
    "tailwindcss": "^3.4.1"
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "phoenix": "file:../deps/phoenix",
    "phoenix_html": "file:../deps/phoenix_html",
    "phoenix_live_view": "file:../deps/phoenix_live_view",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.0",
    "remount": "^1.0.0",
    "topbar": "^2.0.2"
